{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "strict": true,
    "target": "es2022",
    "types": ["cypress", "node", "vite/client"],
    "allowSyntheticDefaultImports": true
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
